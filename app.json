{
  "expo": {
    "name": "N-of-1 Experiments",
    "slug": "nof1-experiments",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "nof1",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#0f172a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nof1.experiments",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0f172a"
      },
      "package": "com.nof1.experiments"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "81b500c4-6a29-4905-ba8e-ac11dfc7cff9"
      }
    }
  }
}
