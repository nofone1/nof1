{
  "name": "nof1-experiments",
  "version": "1.0.0",
  "description": "N-of-1 Experiments App - Track personal supplement and peptide experiments",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "build:ios-simulator": "eas build --profile development --platform ios --local",
    "build:android-emulator": "eas build --profile preview --platform android --local",
    "build:ios-cloud": "eas build --profile development --platform ios",
    "build:android-cloud": "eas build --profile preview --platform android"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/slider": "^5.1.2",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/native-stack": "^7.0.0",
    "expo": "~52.0.0",
    "expo-asset": "~11.0.5",
    "expo-dev-client": "~5.0.20",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-linear-gradient": "~14.0.2",
    "expo-status-bar": "~2.0.0",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@types/react": "~18.3.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.0",
    "typescript": "~5.3.0"
  },
  "private": true
